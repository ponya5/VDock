          "id": "btn-jetbrains-search-everywhere",
          "label": "Search All",
          "secondary_label": "Shift Shift",
          "icon": ["fas", "search"],
          "icon_type": "fontawesome",
          "position": { "row": 0, "col": 0 },
          "size": { "rows": 1, "cols": 1 },
          "shape": "rounded",
          "style": {
            "backgroundColor": "#087cfa",
            "textColor": "#ffffff"
          },
          "tooltip": "Search everywhere (Double Shift)",
          "enabled": true,
          "action": {
            "type": "macro",
            "config": {
              "steps": [
                { "type": "hotkey", "keys": ["shift"] },
                { "type": "delay", "duration": 100 },
                { "type": "hotkey", "keys": ["shift"] }
              ]
            }
          }
        },
